  "text.autoconfig.terramine.option.client.showFirstPersonGloves": "Show first person gloves",
  "text.autoconfig.terramine.option.client.showFirstPersonGloves.@Tooltip[0]": "Whether gloves should display on",
  "text.autoconfig.terramine.option.client.showFirstPersonGloves.@Tooltip[1]": "the player's hand in first person",
  "text.autoconfig.terramine.option.client.showTooltips": "Show tooltips",
  "text.autoconfig.terramine.option.client.showTooltips.@Tooltip[0]": "Whether terramine should have a tooltip explaining their",
  "text.autoconfig.terramine.option.client.showTooltips.@Tooltip[1]": "effect. These can still be found in REI when disabled",
