  "text.autoconfig.terramine.category.general": "General",
  "text.autoconfig.terramine.option.general.disableEvilSpread": "Disable Evil Biome Spread",
  "text.autoconfig.terramine.option.general.disableEvilSpread.@Tooltip[0]": "Disables the spread of the worlds evil biome",
  "text.autoconfig.terramine.option.general.disableEvilSpread.@Tooltip[1]": "Evil type blocks will no longer spread",
  "text.autoconfig.terramine.option.general.disableEvilSpread.@Tooltip[2]": "Use this for big servers to reduce world lag",
  "text.autoconfig.terramine.option.general.evilSpreadRarity": "Evil Spread Rarity",
  "text.autoconfig.terramine.option.general.evilSpreadRarity.@Tooltip[0]": "How rarely evil type blocks spread",
  "text.autoconfig.terramine.option.general.evilSpreadRarity.@Tooltip[1]": "The higher the value the slower the evil spreads",
  "text.autoconfig.terramine.option.general.disableFallingStars": "Disable Falling Stars",
  "text.autoconfig.terramine.option.general.disableFallingStars.@Tooltip": "Disables Falling Stars from randomly falling",
  "text.autoconfig.terramine.option.general.fallingStarRarity": "Falling Star Rarity",
  "text.autoconfig.terramine.option.general.fallingStarRarity.@Tooltip[0]": "How rarely Falling Stars will fall",
  "text.autoconfig.terramine.option.general.fallingStarRarity.@Tooltip[1]": "The higher the value the more often stars will fall",
  "text.autoconfig.terramine.option.general.disableMeteorites": "Disable Meteorites",
  "text.autoconfig.terramine.option.general.disableMeteorites.@Tooltip": "Meteorites will no longer fall onto the world",
  "text.autoconfig.terramine.option.general.meteoriteRarity": "Meteorite Rarity",
  "text.autoconfig.terramine.option.general.meteoriteRarity.@Tooltip[0]": "How rarely Meteorites will fall onto the world",
  "text.autoconfig.terramine.option.general.meteoriteRarity.@Tooltip[1]": "The higher the value the more often meteorites will fall",
  "text.autoconfig.terramine.option.general.meteoriteRarity.@Tooltip[2]": "(Meteorites still only spawn at midnight)",
  "text.autoconfig.terramine.option.general.disableHardcoreExtraAccessory": "Disable extra Hardcore accessory slot",
  "text.autoconfig.terramine.option.general.disableHardcoreExtraAccessory.@Tooltip[0]": "Disables the extra accessory slot given to the player from playing on Hardcore difficulty",
  "text.autoconfig.terramine.option.general.disableHardcoreExtraAccessory.@Tooltip[1]": "(if the slot has already been added it will be removed)",
  "text.autoconfig.terramine.option.general.disableDemonHeart": "Disable Demon Heart",
  "text.autoconfig.terramine.option.general.disableDemonHeart.@Tooltip": "Disables the functionality of the Demon Heart, item can still be obtained",
  "text.autoconfig.terramine.option.general.setExtraAccessorySlots": "Set extra accessory slots",
  "text.autoconfig.terramine.option.general.setExtraAccessorySlots.@Tooltip[0]": "Override how many extra accessory slots the player has",
  "text.autoconfig.terramine.option.general.setExtraAccessorySlots.@Tooltip[1]": "(the Demon Heart will still add a slot unless disabled)",
